<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane fx:id="context" prefHeight="259.0" prefWidth="378.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.devstack.lms.controller.SignupFormController">
   <children>
      <Button layoutX="23.0" layoutY="170.0" mnemonicParsing="false" onAction="#createNewAccountOnAction" prefHeight="33.0" prefWidth="325.0" text="Register" />
      <Label layoutX="24.0" layoutY="24.0" text="Sign Up">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="24.0" layoutY="71.0" prefHeight="33.0" prefWidth="325.0" promptText="Username" />
      <PasswordField fx:id="txtPassword" layoutX="24.0" layoutY="121.0" prefHeight="33.0" prefWidth="325.0" promptText="Password" />
      <Hyperlink layoutX="135.0" layoutY="217.0" onAction="#navigateToLoginFormOnAction" text="Already have an account" />
   </children>
</AnchorPane>
