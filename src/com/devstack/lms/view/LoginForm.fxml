<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="context" prefHeight="244.0" prefWidth="353.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.devstack.lms.controller.LoginFormController">
   <children>
      <Button layoutX="13.0" layoutY="160.0" mnemonicParsing="false" onAction="#openDashboardFormOnAction" prefHeight="33.0" prefWidth="325.0" text="Login" />
      <Label layoutX="14.0" layoutY="14.0" text="Login">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <TextField fx:id="txtUsername" layoutX="14.0" layoutY="61.0" prefHeight="33.0" prefWidth="325.0" promptText="Username" text="admin" />
      <PasswordField fx:id="txtPassword" layoutX="14.0" layoutY="111.0" prefHeight="33.0" prefWidth="325.0" promptText="Password" text="1234" />
      <Hyperlink layoutX="125.0" layoutY="207.0" onAction="#navigateToRegisterFormOnAction" text="create an account" />
   </children>
</AnchorPane>
